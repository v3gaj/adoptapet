$("<%= j (render :partial => 'posts/post') %>").insertAfter($('.post-details').last());
$('.add_five_posts').html('<%= link_to "Mostrar mas publicaciones!", add_five_path(count: @count, pet_id: @pet.slug), remote: :true, :class => "btn btn-primary add_five_link" %>');
<% if session[:count] == @count %>
	$('.add_five_posts').remove();
<% end %>